<script>
function get_action(){
	var e = document.getElementById("searchbasis");
	var searchbasis = e.options[e.selectedIndex].value;
	var action="";
	if(searchbasis=="p_type"){
		document.getElementById("srchFld").setAttribute("name", "ptype19");
		action="/productTypeFromCategory";
	}
	else{
		document.getElementById("srchFld").setAttribute("name", "p_name");
		action="/searchproductname";
	}
	return action;
}
</script>
